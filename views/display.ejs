<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/display.css">
    <title>Display Paper</title>
</head>
<body>
    <div class="navbar">
        <center><h1>Generated Question Paper</h1></center>
        <a href="/">Home</a>
    </div>
    
    
    <br>
    <table>
        <tr>
            
            <th>Question</th>
            <th>Topic</th>
            <th>Difficulty</th>
            <th>Marks</th>
        </tr>
        
            <%questionPaper.forEach(q => {%>
                <tr>
                <td><%=q.question%></td>
                <td><%=q.topic%></td>
                <td><%=q.difficulty%></td>
                <td><%=q.marks%></td>
            </tr>
            <%});%>
        
    </table>
    
</body>
</html>